{
    "name": "youcanshop/sdk",
    "description": "YouCan Shop SDK",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "YouCan\\Shop\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "nesbot/carbon": "^2.68"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "suggest": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "scripts": {
        "phpunit": "phpunit --verbose",
        "test": [
            "@phpunit"
        ]
    }
}
